        <Image Grid.ColumnSpan="19" Grid.RowSpan="25" Grid.Column="0" Grid.Row="0" Source="Resources/1.bmp" Stretch="Fill" Margin="0,-25,3.165,-0.286"  />
        <Button Content="查询"  Background="White" Grid.Column="21" HorizontalAlignment="Left" Margin="10.336,10.143,0,0" Grid.Row="19" VerticalAlignment="Top" Width="80" Height="27" Click="Button_Find" Grid.RowSpan="4"/>
        <Button Content="导出" Background="White" Grid.Column="19" HorizontalAlignment="Left" Margin="26.835,1.857,0,0" Grid.Row="20" VerticalAlignment="Top" Width="81" Height="27" Grid.ColumnSpan="2" Grid.RowSpan="3"/>
