        <ListBox Name="listTags" Grid.ColumnSpan="2" Grid.Column="20" HorizontalAlignment="Left" Height="264" Margin="6.264,9.76,-6.05,0" Grid.RowSpan="10" VerticalAlignment="Top" Width="360" Grid.Row="8"/>
        <Button Content="暂停" Background="White" Grid.Column="20" HorizontalAlignment="Left" Margin="146.264,9.872,0,0" VerticalAlignment="Top" Width="70" Height="30" Click="buttonStop_Click" Grid.Row="2" Grid.ColumnSpan="2" Grid.RowSpan="4"/>
        <Button Content="清除" Background="White" Grid.Column="21" Margin="0,9.872,14.95,9.954" Click="buttonClear_Click" Grid.Row="2" HorizontalAlignment="Right" Width="69" Grid.RowSpan="5"/>

